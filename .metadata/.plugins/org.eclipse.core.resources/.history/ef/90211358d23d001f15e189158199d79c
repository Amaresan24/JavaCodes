package java_1;
import java.util.*;

public class optional_class1 {
public static void main(String args[]) {
	
	//String word = "hai";
	
	 String word = "hai";
	  
Optional<String> op = Optional.of(word);	
Optional<String> op1 = Optional.ofNullable(word);	
Optional<String> op2 = Optional.empty();	





	
System.out.println(op.isEmpty());
System.out.println(op1.isPresent());
//System.out.println(op1.ofNullable(word));


}	

}
