package loops;

public class forloopcon2 {
public static void main(String args[]) {
	
String name = "education";

for(int i = 0 ; i<name.length(); i++) {
	
	char check = name.charAt(i);
	
if(check == 'a' || check == 'e' || check =='i' || check =='o' || check == 'u') {
	continue;	
}else {
	
	System.out.println(check);
}
	}
	



