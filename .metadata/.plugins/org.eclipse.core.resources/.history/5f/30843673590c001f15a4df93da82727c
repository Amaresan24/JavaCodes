package ReviewQuestions;

public class whilePanlidrom {
public static void main(String args[]) {
	
int i = 111;
int result = 0; 
int temp = 0; 
while(i < 500) {
	int num = i;
	while(num > 0) {
	temp = num % 10;
	result = (result * 10) + temp;
	num = num /10;
		}
	if( result == i) {
		System.out.println(i);
		//i++;
	}
	
}
	
	
	
}	
}
