package Final_keywords;

public class Static_KeyWords {
	
	static int a = 10; /// -----------------> static variable used 
	
	
	
	public static void  getName() {
		
		System.out.println("Amaresh");
		
	}

	public static void main(String[] args) {

	//int a = 10; 
	System.out.println(a);
	
	
	
	}

}
